//problem7
const sign = function (num1, num2, num3) {

  if (num1 === 0 || num2 === 0 || num3 === 0) {
    console.log("unsigned!");
   }

  else if ((num1 < 0 && num2 < 0 && num3 < 0) || (num1 < 0 && num2 > 0 && num3 > 0) || (num2 < 0 && num1 > 0 && num3 > 0) || (num3 < 0 && num2 > 0 && num1 > 0)) {
    console.log("-");
  }

  else {
    console.log("+");
  }

}

//problem8 not finished
const quadratic = function (a, b, c) {
  if (a === 0) {
    console.log("Enter valid constants");
  }

  else {
    let d = b**2 - (4*a*c);
    if(d < 0) {
      console.log("solution does not exist");
    }
    else {
      let r1 = ((-1) * b - Math.sqrt(d)) / 2*a;
      let r2 = ((-1) * b + Math.sqrt(d)) / 2*a;
        if(r1 === r2) {
          console.log(r1);
        }

        else {
          console.log("solutions are " + r1 + " and " + r2);
        }
      }
    }
  }

