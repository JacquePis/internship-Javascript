  if (number < 0) {
    console.log("the number is negative");
    }
 
  else {
     console.log(number + Number(String(number) + String(number)) + Number(String(number) + String(number) + String(number)));
    }
}

//problem4 not finished
const move_last_digit = function (number) {
  if (number < 0) {
  return 0;
  }

  else {
    if (number % 10 === 0) {
      return number;
    }

    else {
      var last_digit = number % 10;
      console.log(String(last_digit) + String(number - last_digit));
    }
  }
}

